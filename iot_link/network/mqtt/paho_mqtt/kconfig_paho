    int "Paho connect timeout:ms"
    default 10000
  
config PAHO_CMD_TIMEOUT
    int "Paho subscribe/publish timeout:ms"
    default 10000
    
config PAHO_LOOPTIMEOUT
    int "Paho socket recive timeout:ms"
    default 10
    
config PAHO_SNDBUF_SIZE
    int "Paho send buf:bytes"
    default 2048
    
config PAHO_RCVBUF_SIZE
    int "Paho recv buf:bytes"
    default 2048 
           
      
         
